{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    "admins":{
        ".read": "auth != null",
        ".write": "root.child('admins').child(auth.token.email.replace('.', '%2E')).exists()"
    },
    "weekly_codes": {
        ".read": true,
        ".write": "root.child('admins').child(auth.token.email.replace('.', '%2E')).exists()"
    },
     ".read": true,
     ".write": "auth != null"
  }
}
